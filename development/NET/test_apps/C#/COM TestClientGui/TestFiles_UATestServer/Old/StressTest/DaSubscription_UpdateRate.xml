	<DaSubscriptionWaitForNotif>
		<Subscription>DaSubscription.Subscription</Subscription>
		<ExpectValues>false</ExpectValues>
		<WaitTime>3000</WaitTime>
	</DaSubscriptionWaitForNotif>

	<DaItemsRemove>

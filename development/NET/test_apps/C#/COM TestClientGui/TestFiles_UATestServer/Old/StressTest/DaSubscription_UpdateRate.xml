      <string>ns*2;s*Dynamic.UInt8</string>
      <!--ByteValue-->
      <string>ns*2;s*Dynamic.Int16</string>
      <!--Int16Value-->
      <string>ns*2;s*Dynamic.UInt16</string>
      <!--UInt16Value-->
      <string>ns*2;s*Dynamic.Int32</string>
